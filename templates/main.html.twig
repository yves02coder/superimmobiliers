<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!-- Axentix CSS minified version -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.2.1/dist/css/axentix.min.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet" >
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            <script>
               window.onload= () =>{
                   document.querySelector("#immobilier_contact_title")
               }
               let modal = new Modal('#modale-contact');
            </script>
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body class="mt-5">
    <nav class="navbar navbar-expand-lg navbar-light bg-secondary h-15">
        
        <div class="container-fluid">

            <a class="navbar-brand" href="#"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    

                    <li class="nav-item m-5">
                        <a class="nav-link  " href="{{ path('app_register') }}" tabindex="-1" aria-disabled="true"><i class="fa fa-user-circle fa-2x text-light" aria-hidden="true"></i>user</a>
                    </li>
                    <li class="nav-item m-5">
                        <a class="nav-link  " href="{{ path('admin') }}" tabindex="-1" aria-disabled="true"><i class="fa fa-tachometer fa-2x text-light" aria-hidden="true"></i>Dashboard</a>
                    </li>
                 <li class="nav-item m-5">
                        <a class="nav-link  " href="{{ path('app_login') }}" tabindex="-1" aria-disabled="true"><i class="fa fa-creative-commons fa-2x text-light" aria-hidden="true"></i>connexion</a>
                    </li>
                </ul>
                {#<form class="d-flex">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-light" type="submit">Search</button>
                </form>#}
            </div>
        </div>
    </nav>
    <div class="container">
        {% block body %}{% endblock %}
    </div>
    <header class="header">
<div class="p-5 text-center bg-image" style="
		          background-image: url('https://www.barnes-lyon.com/uploads/accommodations/1363/pictures/26851/show/barnes-lyon-maison-villa-seyssuel.jpg?1592985640');
		          height: 600px;background-repeat: no-repeat;background-position: center;background-size: cover;
		        ">
	<div class="mask" style="background-color: rgba(0, 0, 0, 0.6);">
		<div class="d-flex justify-content-center align-items-center h-100">
			<div class="text-white">
				<h1 class="mb-3 text-white">Agence</h1>
				<h1 class="mb-3 text-danger">Y<b class="text-primary">'MMOBILIER</b>
				</h1>
				<a class="btn btn-outline-light btn-lg" href="{{ path("immobilier_index") }}" role="button">voir les annonces</a>
				<div class="container text-success mt-5">
					<h4 class="text-light">
						<b>Agence Immobilier</b>

					</h4>
          

					<p class="mt-3 text-light">
						mail:no_reply@immobilier.com
					</p>
					<p class="mt-3 text-light">
						Numéro: 00 01 02 04
					</p>
				</div>

			</div>


		</div>

	</div>

</div>


    
    </header>

    <footer class="text-center text-white" style="background-color: #f1f1f1;">
        <!-- Grid container -->
        <div class="container pt-4">
            <!-- Section: Social media -->
            <section class="mb-4">
                <!-- Facebook -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-facebook-f"></i
                    ></a>

                <!-- Twitter -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-twitter"></i
                    ></a>

                <!-- Google -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-google"></i
                    ></a>

                <!-- Instagram -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-instagram"></i
                    ></a>

                <!-- Linkedin -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-linkedin"></i
                    ></a>
                <!-- Github -->
                <a
                        class="btn btn-link btn-floating btn-lg text-dark m-1"
                        href="#!"
                        role="button"
                        data-mdb-ripple-color="dark"
                ><i class="fa fa-github"></i
                    ></a>
            </section>
            <!-- Section: Social media -->
        </div>
        <!-- Grid container -->

        <!-- Copyright -->
        <div class="text-center text-dark p-3" style="background-color: rgba(0, 0, 0, 0.2);">
            © 2020 Copyright:
            <a class="text-dark" href="https://mdbootstrap.com/">MDBootstrap.com</a>
        </div>
        <!-- Copyright -->
    </footer>


        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.min.js" integrity="sha384-lpyLfhYuitXl2zRZ5Bn2fqnhNAKOAaM/0Kr9laMspuaMiZfGmfwRNFh8HlMy49eQ" crossorigin="anonymous"></script>
    <!-- Axentix JS minified version -->
        <script src="https://cdn.jsdelivr.net/npm/axentix@1.2.1/dist/js/axentix.min.js"></script>
    <!-- Grix system -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.2.1/dist/css/grix.min.css">

    </body>
</html>
